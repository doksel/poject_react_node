<% layout('layout/wrapper') -%>
<div class="wrapper users">
    <div><h3>All users:</h3></div>
    <div class="users_list">
        <% users.forEach((user)=>{ %>
            <div><span>login: </span><span><%= user.login %></span></div>
            <div><span>email: </span><span><%= user.email %></span></div>
            <div><span>date created: </span><span><%= user.createdAt %></span></div>
        <% }) %>
    </div>
</div>